
% A and B are names of matricies
times(A, B, I, J) += m(A, I, K) * m(B, K, J).

% matrix "c" is the product of matricies "a" and "b"
m(c, I, J) += times(a, b, I, J).

% matrix "a" = [ 1 0 ;
%                0 1 ]
m(a, 1, 1) += 1.
m(a, 1, 2) += 0 .
m(a, 2, 1) += 0 .
m(a, 2, 2) += 1.

% matrix "b" = [ 3 0 ;
%                0 2 ]
m(b, 1, 1) += 3.
m(b, 1, 2) += 0 .
m(b, 2, 1) += 0 .
m(b, 2, 2) += 2.
